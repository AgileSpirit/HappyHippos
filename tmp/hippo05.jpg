<script>
$('#imgArticle').load(function(){

}).each(function() {
    //ensure image load is fired. Fixes opera loading bug
	imgWidth = $('#imgArticle').width();
	imgHeight = $('#imgArticle').height();

	if(imgWidth && imgHeight){

		tailleImage();

	}else{

		setTimeout(tailleImage, 2500);
	}
});

function tailleImage(){

	imgWidth = $('#imgArticle').width();
	imgHeight = $('#imgArticle').height();

	if(imgWidth != 1024){
		imgTxtSize = 'Taille de cet aperçu : <a href="/picture/thumb/'+ imgWidth + 'px-hippo_et_bb.jpg">'+ imgWidth +' x '+ imgHeight +' pixels</a>.';
		$('#sizeImg').prepend(imgTxtSize);
	}
}
</script>
